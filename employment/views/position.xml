<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="position_act" model="ir.actions.act_window">
            <field name="name">Vị Trí Đào Tạo</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">position.educate</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Tạo Mới Vị trí Đào Tạo
                </p>
            </field>
        </record>
        <record id="mylib_position_tree" model="ir.ui.view">
            <field name="name">Vị trí đào tạo</field>
            <field name="model">position.educate</field>
            <field name="arch" type="xml">
                <tree string ='Vị Trí Đào Tạo'>
                    <field name="name"/>
                    <field name="date_position"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>